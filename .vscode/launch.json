    "version": "0.2.0",
    "configurations": [
        {
            "name": "antialias.py eval",
            "type": "debugpy",
            "request": "launch",
            "program": "antialias.py",
            "console": "integratedTerminal",
            "args": [
                "--config",
                ".config/config.json",
                "eval",
                "--",
                "${command:pickArgs}"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "antialias.py list",
            "type": "debugpy",
            "request": "launch",
            "program": "antialias.py",
            "console": "integratedTerminal",
            "args": [
                "--config",
                ".config/config.json",
                "list",
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}